<%- include("header") -%>
<div class="box" id="heading">
  <h1><%= titleName %></h1>
</div>
<div class="box">
  <% items.forEach((value)=>{ %>
  <div class="item">
    <input type="checkbox" />
    <p class="item"><%= value%></p>
  </div>
  <%})%>
  <form class="item" action="/" method="post">
    <input type="text" name="newItem" placeholder="New item" />
    <button type="submit" name="button" value="<%= titleName %>">+</button>
  </form>
</div>
<%- include("footer") -%>
